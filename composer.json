{
  "name": "tianfuunion/mark-coss",
  "description": "The Tianfu Union Mark Engine Coss.",
  "keywords": [
    "tianfuunion",
    "markengine",
    "mark-coss"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://auth.tianfu.ink",
  "authors": [
    {
      "name": "Mark Young",
      "email": "mark@tianfuunion.cn"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "psr/log": "~1.0",
    "psr/container": "~1.0",
    "psr/simple-cache": "^1.0",
    "ext-fileinfo": "*",
    "tianfuunion/mark-engine": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0|~8.0",
    "php-coveralls/php-coveralls": "^2.2",
    "phpstan/phpstan": "^0.12"
  },
  "autoload": {
    "psr-4": {
      "mark\\": "src"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer/"
    }
  }
}